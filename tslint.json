{
  "extends": [
    "tslint:latest",
    "tslint-config-prettier"
  ],
  "rules": {
    "no-any": false,
    "semicolon": [true, "always"],
    "space-before-function-paren": false,
    "any": false,
    "no-var-requires": false,
    "object-literal-sort-keys": false,
    "no-console": false,
    "interface-name" : [true, "never-prefix"]
  }
}